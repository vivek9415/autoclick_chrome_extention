{
    "manifest_version": 3,
    "name": "Auto Clicker Vivek Project",
    "version": "1.0",
    "description": "An auto-clicker extension with a UI to control clicks.",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  